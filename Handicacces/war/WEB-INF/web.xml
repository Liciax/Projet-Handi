<?xml version="1.0" encoding="utf-8" standalone="no"?><web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.5" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">

<filter>
        <filter-name>ObjectifyFilter</filter-name>
        <filter-class>com.googlecode.objectify.ObjectifyFilter</filter-class>
</filter>
<filter-mapping>
        <filter-name>ObjectifyFilter</filter-name>
        <url-pattern>/*</url-pattern>
</filter-mapping>

	<servlet>
		<servlet-name>Handicacces</servlet-name>
		<servlet-class>fr.univnantes.servlet.HandicaccesServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Handicacces</servlet-name>
		<url-pattern>/handicacces</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	
	<servlet>
		<servlet-name>Website</servlet-name>
		<servlet-class>fr.univnantes.servlet.WebsiteServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Website</servlet-name>
		<url-pattern>/website</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Amenagement</servlet-name>
		<servlet-class>fr.univnantes.servlet.AmenagementServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Amenagement</servlet-name>
		<url-pattern>/amenagement</url-pattern>
	</servlet-mapping>
	
		<servlet>
		<servlet-name>AjoutRenseignements</servlet-name>
		<servlet-class>fr.univnantes.servlet.AjoutRenseignementsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AjoutRenseignements</servlet-name>
		<url-pattern>/ajout</url-pattern>
	</servlet-mapping>
	
 <servlet>
  <servlet-name>SystemServiceServlet</servlet-name>
  <servlet-class>com.google.api.server.spi.SystemServiceServlet</servlet-class>
  <init-param>
   <param-name>services</param-name>
   <param-value>fr.unantes.api.WebsiteEndpoint,test.ScoreEntityEndpoint</param-value>
  </init-param>
 </servlet>
 <servlet-mapping>
  <servlet-name>SystemServiceServlet</servlet-name>
  <url-pattern>/_ah/spi/*</url-pattern>
 </servlet-mapping>
</web-app>